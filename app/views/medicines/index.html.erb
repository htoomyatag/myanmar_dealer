<p id="notice"><%= notice %></p>

<h1>Listing Medicines</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>Certification</th>
      <th>Ingredient</th>
      <th>Usage</th>
      <th>Made by country</th>
      <th>Description</th>
      <th>Caution</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicines.each do |medicine| %>
      <tr>
        <td><%= medicine.title %></td>
        <td><%= medicine.category %></td>
        <td><%= medicine.certification %></td>
        <td><%= medicine.ingredient %></td>
        <td><%= medicine.usage %></td>
        <td><%= medicine.made_by_country %></td>
        <td><%= medicine.description %></td>
        <td><%= medicine.caution %></td>
        <td><%= link_to 'Show', medicine %></td>
        <td><%= link_to 'Edit', edit_medicine_path(medicine) %></td>
        <td><%= link_to 'Destroy', medicine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medicine', new_medicine_path %>
