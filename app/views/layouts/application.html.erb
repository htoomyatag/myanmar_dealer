            <h5>Follow Us</h5>
            <ul>
              <li><a href="#">Facebook</a></li>
              <li><a href="#">Twitter</a></li>
              <li><a href="#">RSS</a></li>
              <li><a href="#">YouTube</a></li>
            </ul>
          </div>
        <!-- Follow Us Links Ends -->
        <!-- Contact Us Starts -->
          <div class="col-md-4 col-sm-12 last">
            <h5>Contact Us</h5>
            <ul>
              <li>Myanmar Dealer.com</li>
              <li>
                No.A (21), U Wisara Road, Industrial Zone, North Dagon, Yangon, Myanmar.
              </li>
              <li>
                Email: <a href="#">info@myanmardealer.com</a>
              </li>               
            </ul>
            <h4 class="lead">
              Tel: <span> (+959) 972646904</span>
            </h4>
          </div>
        <!-- Contact Us Ends -->
      </div>
    <!-- Container Ends -->
    </div>
  <!-- Footer Links Ends -->
  <!-- Copyright Area Starts -->
    <div class="copyright">
    <!-- Container Starts -->
      <div class="container">
      <!-- Starts -->
        <p class="pull-left">
          &copy; 2016 Myanmar Dealers. Powered By <a href="http://www.hnttechs.com">H&Ttechs</a>
        </p>
      <!-- Ends -->
      <!-- Payment Gateway Links Starts -->
        <ul class="pull-right list-inline">
          <li>
            <img src="/assets/payment-icon/mpu.jpg" alt="PaymentGateway" />
          </li>
        
        </ul>
      <!-- Payment Gateway Links Ends -->
      </div>
    <!-- Container Ends -->
    </div>
  <!-- Copyright Area Ends -->
  </footer>

</body>
</html>






