<div class="container">
<p id="notice"><%= notice %></p>

<h1>Listing Fashion Bags</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Quantity</th>
      <th>Made by country</th>
      <th>Made with</th>
      <th>Description</th>
      <th>Bag type</th>
      <th>Size</th>
      <th>Brand</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fashion_bags.each do |fashion_bag| %>
      <tr>
        <td><%= fashion_bag.title %></td>
        <td><%= fashion_bag.price %></td>
        <td><%= fashion_bag.quantity %></td>
        <td><%= fashion_bag.made_by_country %></td>
        <td><%= fashion_bag.made_with %></td>
        <td><%= fashion_bag.description %></td>
        <td><%= fashion_bag.bag_type %></td>
        <td><%= fashion_bag.size %></td>
        <td><%= fashion_bag.brand %></td>
        <td><%= link_to 'Show', fashion_bag %></td>
        <td><%= link_to 'Edit', edit_fashion_bag_path(fashion_bag) %></td>
        <td><%= link_to 'Destroy', fashion_bag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fashion bag', new_fashion_bag_path %>
</div>
