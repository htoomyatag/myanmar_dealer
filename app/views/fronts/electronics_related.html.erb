											<div class="price">
												<span class="price-new"><%= electrical_equipment.price.to_i %> <%=t :MMK %></span> 
												<span class="price-old"><%= electrical_equipment.price.to_i + 15 %> <%=t :MMK %></span>
											</div>
											<div class="cart-button">
											
											<%= button_to line_items_path(:mmdealer_code => "eletronic_related"+electrical_equipment.id.to_s, :user_id => electrical_equipment.user_id), :class =>"btn btn-cart" do %>
											   
											  
