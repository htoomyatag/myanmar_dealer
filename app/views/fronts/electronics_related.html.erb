										<div class="image">
											<% if !electrical_equipment.avatar1_file_name.nil?  %>
								                <%= image_tag electrical_equipment.avatar1.url, :id => "product_crop_image"  %>
								            <% else  %>      
												<img src="/assets/no_image.jpg" alt="product" class="img-responsive" />
								            <% end %>
										</div>
										<div class="caption">
											<h6>
												
												<%= link_to electrical_equipment.title, full_productinfo_electrical_equipments_path(:id => electrical_equipment) %>
