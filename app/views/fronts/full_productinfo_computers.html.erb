								<option value="2000"> 
										Mayangon
										</option>
								<option value="2000"> 
										Yankin
										</option>
								<option value="2000"> 
										Tharkayta
								</option>
								<!--  -->

								<option value="2500"> 
								Dagon myothit(south)
								</option>
								<option value="2500"> 	
								Dagon myothit(north)	
								</option>
								<option value="2500"> 
								Dagon myothit(east)	
