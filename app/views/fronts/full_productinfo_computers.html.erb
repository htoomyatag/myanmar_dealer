					</li>
					<li>
						<a href="/assets/product-images/big-pimg4.jpg">
							<img src="/assets/product-images/thumb3.jpg" alt="Image" class="img-responsive thumbnail" />
						</a>
					</li>
					<li>
						<a href="/assets/product-images/big-pimg4.jpg">
							<img src="/assets/product-images/thumb4.jpg" alt="Image" class="img-responsive thumbnail" />
						</a>
					</li>
				</ul>
			</div>
		<!-- Left Ends -->
		<!-- Right Starts -->
			<div class="col-sm-8 product-details">
				<div class="panel-smart">
				<!-- Product Name Starts -->
					<h2><%= @product.title rescue "-" %></h2>
					 
				<!-- Product Name Ends -->
					<hr />
				<!-- Manufacturer Starts -->
					<ul class="list-unstyled manufacturer">
					    <li>
							<span>Model:</span> <%= @product.modal_number rescue "-"%>
						</li>
						 
						 <li>
							<span>Category:</span> <%= @product.category rescue "-"%>
						</li>
					    <li>
							<span>Brand:</span> <%= @product.brand rescue "-"%>
						</li>
						<li>
							<span>Color:</span> <%= @product.color rescue "-" %>
						</li>
						<li><span>Size:</span> <%= @product.size rescue "-" %>
						</li>
						
						<li><span>Weight:</span>  <%= @product.weight rescue "-" %>
						</li>

						<li><span>Thickness:</span>  <%= @product.thickness rescue "-" %>
						</li>

						<li>
							<span>Availability:</span> <strong class="label label-danger">Out Of Stock</strong>
						</li>
						<li>
							<span>Made in:</span> <%= @product.made_by_country rescue "-" %>
						</li>
						<li>
							<span>Made with:</span> <%= @product.made_with rescue "-" %>
						</li>
						<li>
							<span>OS:</span> <%= @product.operation_system rescue "-"%>
						</li>
						<li><span>Feature:</span>  <%= @product.feature rescue "-" %>
						</li>
					</ul>
				<!-- Manufacturer Ends -->
					<hr />
				<!-- Price Starts -->
					<div class="price">
						<span class="price-head">Price :</span>
						<span class="price-new"><%= @product.price.to_i %> MMK
						</span> 
						<span class="price-old"><%= @product.price.to_i + 15 %> MMK
						</span> 
						<br/>
						<span class="price-head">Delivery Fees:</span>
						<span class="price-new"> 
							<input id="delivery_fees">
						</span> 
					</div>
				<!-- Price Ends -->
					<hr />
				<!-- Available Options Starts -->
