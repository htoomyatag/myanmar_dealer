<div id="main-container" class="container">
  <!-- Breadcrumb Starts -->
    <ol class="breadcrumb">
      <li><a href="index.html">Home</a></li>
      <li class="active">Register</li>
    </ol>
  <!-- Breadcrumb Ends -->
  <!-- Main Heading Starts -->
    <h2 class="main-heading text-center">
      Register <br />
    </h2>
  <!-- Main Heading Ends -->
  <!-- Registration Section Starts -->
    <section class="registration-area">
      <div class="row">
        <div class="col-sm-6">
        <!-- Registration Block Starts -->
          <div class="panel panel-smart">
            <div class="panel-heading">
              <h3 class="panel-title">Personal Information</h3>
            </div>
            <div class="panel-body">
            <!-- Registration Form Starts -->
              <%= form_for(:user, :url => registration_path(:user)) do |f| %>

              <!-- Personal Information Starts -->
                <div class="form-group">
                  <label for="inputFname" class="col-sm-3 control-label">Full Name :</label>
                  <div class="col-sm-9">
                    <%= f.text_field :name, autofocus: true, :class => "form-control", :Placeholder => "Name" %>
                  </div><br/><br/>
                </div>
               
                <div class="form-group">
                  <label for="inputEmail" class="col-sm-3 control-label">Email :</label>
                  <div class="col-sm-9">
                      <%= f.email_field :email, autofocus: true, :class => "form-control", :Placeholder => "Email" %><br/>
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputPhone" class="col-sm-3 control-label">Phone :</label>
                  <div class="col-sm-9">
                     <%= f.text_field :phone, autofocus: true, :class => "form-control", :Placeholder => "Phone" %><br/>
                  </div>
                </div>

                  <%= f.text_field :user_type_id, autofocus: true, :class => "form-control", :Placeholder => "Phone", :value => 2, :style => "display:none;" %>
               
              <!-- Personal Information Ends -->
               
              <!-- Delivery Information Ends -->
                <h3 class="panel-heading inner">
                  Password
                </h3>
              <!-- Password Area Starts -->
                <div class="form-group">
                  <label for="inputPassword" class="col-sm-3 control-label">Password :</label>
                  <div class="col-sm-9">
                    <%= f.password_field :password, autocomplete: "off", :class=>"form-control", :Placeholder => "Password" %><br/>
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputRePassword" class="col-sm-3 control-label">Re-Password :</label>
                  <div class="col-sm-9">
                    <%= f.password_field :password_confirmation, autocomplete: "off", :class=>"form-control", :Placeholder => "Re-Password" %><br/>
                  </div>
                </div>
               
                <div class="form-group">
                  <div class="col-sm-offset-3 col-sm-9">
                    <div class="checkbox">
                      <label>
                        <input type="checkbox"> I'v read and agreed on Conditions
                      </label>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-sm-offset-3 col-sm-9">
                    <button type="submit" class="btn btn-black">
                      Register
                    </button>
                  </div>
                </div>
              <!-- Password Area Ends -->
              <% end %>
            <!-- Registration Form Starts -->
            </div>              
          </div>
        <!-- Registration Block Ends -->
        </div>
        <div class="col-sm-6">
        
        <!-- Conditions Panel Starts -->
