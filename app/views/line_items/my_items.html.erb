<div class="main-header">
      <div class="container">
        <div class="row">
        <!-- Search Starts -->
          <div class="col-md-3">
            <div id="search">
              <div class="input-group">
                <input type="text" class="form-control input-lg" placeholder="Search">
                <span class="input-group-btn">
                <button class="btn btn-lg" type="button">
                  <i class="fa fa-search"></i>
                </button>
                </span>
              </div>
            </div>  
          </div>
        <!-- Search Ends -->
        <!-- Logo Starts -->
          <div class="col-md-6">
            <div id="logo">
              <a href="#"><img src="/assets/logo.png" title="Spice Shoppe" alt="Spice Shoppe" class="img-responsive" /></a>
            </div>
          </div>
        <!-- Logo Starts -->        
        <!-- Shopping Cart Starts -->
          <div class="col-md-3">
            <div id="cart" class="btn-group btn-block">
              <button type="button" data-toggle="dropdown" class="btn btn-block btn-lg dropdown-toggle">
                <i class="fa fa-shopping-cart"></i>
                <span class="hidden-md">Cart:</span> 
                <span id="cart-total"> <%= (@cart.total_quantity) %> item(s) -   <%= (@cart.total_price) %> <%=t :MMK %></span>
                <i class="fa fa-caret-down"></i>
              </button>
              <ul class="dropdown-menu pull-right">
                  <li>
                  <%= render (@cart.line_items) %>
                </li>
              
                <li>
                  <p class="text-right btn-block1">
                    <a href="/check_out">
                      View Cart
                    </a>
                    <a href="/check_out">
                      Checkout
                    </a>
                  </p>
                </li>

              </ul>
            </div>
          </div>
        <!-- Shopping Cart Ends -->
        </div>
      </div>
    </div>
  <!-- Main Header Ends -->
  <!-- Main Menu Starts -->
    <nav id="main-menu" class="navbar" role="navigation">
      <div class="container">
      <!-- Nav Header Starts -->
        <div class="navbar-header">
          <button type="button" class="btn btn-navbar navbar-toggle" data-toggle="collapse" data-target=".navbar-cat-collapse">
            <span class="sr-only">Toggle Navigation</span>
            <i class="fa fa-bars"></i>
          </button>
        </div>
      <!-- Nav Header Ends -->
      <!-- Navbar Cat collapse Starts -->
        <div class="collapse navbar-collapse navbar-cat-collapse">
          <ul class="nav navbar-nav">
                    <li>
              <a href="/product_by_category?category=Apparel-Textiles-Accessories" class="btn btn-1">
                  Accessories
              </a>

            </li>
            <li>
              <a href="/product_by_category?category=Electronics" class="btn btn-1">
                  Electronics
              </a>
            </li>
            <li>
              <a href="/product_by_category?category=Health-Beauty" class="btn btn-1">
                  Health & Beauty
              </a>
            </li>
            <li>
              <a href="/product_by_category?category=Bags-Shoes-Accessories" class="btn btn-1">
                  Fashions
              </a>
            </li>

            <!--  -->
            <li>
              <a href="/product_by_category?category=Agriculture-Food" class="btn btn-1">
                  Agricultures 
              </a>
            </li>
              <li>
              <a href="/product_by_category?category=Packaging-Advertising-Office" class="btn btn-1">
                  Office
              </a>
            </li>
            
              <li>
              <a href="/product_by_category?category=Gifts-Sports-Toys" class="btn btn-1">
                  Sports
              </a>
            </li>
              <li>
              <a href="/product_by_category?category=Auto-Transportation" class="btn btn-1">
                  Auto Transportation
              </a>
            </li>
          </ul>
        </div>
      <!-- Navbar Cat collapse Ends -->
      </div>
    </nav>
  <!-- Main Menu Ends -->
  </header>

<div id="main-container" class="container">

<br/><br/>

<div class="col-md-12">
<div class="panel panel-default">
  <div class="panel-body">
     <p>MyOrder details</p>
     <hr/>

        <table class="table table-striped">
          <thead>
            <tr>
              <th>Product</th>
              <th>Quantity</th>
            </tr>
          </thead>

          <tbody>
            <% @line_items.each do |line_item| %>
              <tr>
                <td><%= line_item.product.title %></td>
                <td><%= line_item.quantity %></td>
               
              </tr>
            <% end %>
          </tbody>
        </table>
  </div>
</div>
</div>

</div>



