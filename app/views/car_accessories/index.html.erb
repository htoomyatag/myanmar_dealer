<p id="notice"><%= notice %></p>

<h1>Listing Car Accessories</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Quantity</th>
      <th>Size</th>
      <th>Weight</th>
      <th>Made by country</th>
      <th>Made with</th>
      <th>Description</th>
      <th>Brand</th>
      <th>Specification</th>
      <th>Modal number</th>
      <th>Color</th>
      <th>Feature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_accessories.each do |car_accessory| %>
      <tr>
        <td><%= car_accessory.title %></td>
        <td><%= car_accessory.price %></td>
        <td><%= car_accessory.quantity %></td>
        <td><%= car_accessory.size %></td>
        <td><%= car_accessory.weight %></td>
        <td><%= car_accessory.made_by_country %></td>
        <td><%= car_accessory.made_with %></td>
        <td><%= car_accessory.description %></td>
        <td><%= car_accessory.brand %></td>
        <td><%= car_accessory.specification %></td>
        <td><%= car_accessory.modal_number %></td>
        <td><%= car_accessory.color %></td>
        <td><%= car_accessory.feature %></td>
        <td><%= link_to 'Show', car_accessory %></td>
        <td><%= link_to 'Edit', edit_car_accessory_path(car_accessory) %></td>
        <td><%= link_to 'Destroy', car_accessory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car accessory', new_car_accessory_path %>
